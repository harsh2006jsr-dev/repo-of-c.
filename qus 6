
#include<stdio.h>
int main()
{
    double a,b,temp;
    printf("Entre a number (A):\n");
    scanf("%lf",&a);
    printf("Entre a number(B):\n");
    scanf("%lf",&b);
     
    printf("\n----before swapping----\n");
    printf("A=%.2lf\n",a);
    printf("B=%.2lf\n",b);
    
    temp=a;
    a=b;
    b=temp;
    
    printf("\n----after swapping----\n");
    printf("A=%.2lf\n",a);
    printf("B=%.2lf\n",b);
    return 0;
}