Q91: Remove all vowels from a string.

/*
Sample Test Cases:
Input 1:
education
Output 1:
dctn

*/
#include <stdio.h>
#include <string.h>
void removeVowels(char *str) 
{
    char newStr[100]; 
    int i, j = 0;
    for (i = 0; str[i] != '\0'; i++) 
    {
        char ch = str[i];
        if (ch != 'a' && ch != 'e' && ch != 'i' && ch != 'o' && ch != 'u' &&
            ch != 'A' && ch != 'E' && ch != 'I' && ch != 'O' && ch != 'U') 
            {
            newStr[j] = ch;
            j++;
        }
    }
    newStr[j] = '\0';
    strcpy(str, newStr);
}
int main() 
{
    char str[] = "education";
    removeVowels(str);
    printf("%s\n", str); 
    return 0;
}