#include <stdio.h>
#include <math.h>

int main() 
{
    int n, first_digit, last_digit, num_digits, power_of_10;
    printf("Enter a number: ");
    scanf("%d", &n);
    if (n >= 0 && n <= 9) 
    {
        printf("Swapped number: %d\n", n);
        return 0;
    }
    last_digit = n % 10;
    num_digits = floor(log10(n)) + 1;
    power_of_10 = pow(10, num_digits - 1);
    first_digit = n / power_of_10;
    n = n % power_of_10;
    n = n / 10;
    n = n * 10 + first_digit;
    n = last_digit * power_of_10 + n;
    printf("Swapped number: %d\n", n);
    return 0;
}