#include <stdio.h>
void findPreviousGreaterBruteForce(int arr[], int n) 
{
    if (n <= 0) 
    {
        printf("\n"); 
        return;
    }
    for (int i = 0; i < n; i++) 
    {
        int previousGreater = -1; 
        for (int j = i - 1; j >= 0; j--) 
        {
            if (arr[j] > arr[i]) 
            {
                previousGreater = arr[j];
                break; 
            }
        }
        if (i > 0) 
        {
            printf(", ");
        }
        printf("%d", previousGreater);
    }
    printf("\n"); 
}
int main() 
{
    int arr1[] = {1, 3, 2, 4};
    int n1 = sizeof(arr1) / sizeof(arr1[0]);
    printf("Input 1: arr = [1, 3, 2, 4]\n");
    printf("Output 1:\n");
    findPreviousGreaterBruteForce(arr1, n1);
    int arr2[] = {6, 8, 0, 1, 3};
    int n2 = sizeof(arr2) / sizeof(arr2[0]);
    printf("\nInput 2: arr = [6, 8, 0, 1, 3]\n");
    printf("Output 2:\n");
    findPreviousGreaterBruteForce(arr2, n2);
    int arr3[] = {1, 2, 3, 5};
    int n3 = sizeof(arr3) / sizeof(arr3[0]);
    printf("\nInput 3: arr = [1, 2, 3, 5]\n");
    printf("Output 3:\n");
    findPreviousGreaterBruteForce(arr3, n3);
    int arr4[] = {5, 4, 3, 1};
    int n4 = sizeof(arr4) / sizeof(arr4[0]);
    printf("\nInput 4: arr = [5, 4, 3, 1]\n");
    printf("Output 4:\n");
    findPreviousGreaterBruteForce(arr4, n4);
    return 0;
}